class Node {
  // Initialize a linkedlist node
  fun init(val, next) {
    this.val = val;
    this.next = next;
  }

  // Traverse the linked-list and print it in a string
  fun toString() {
    var buf = "[";
    var node = this;
    while (node != nil) {
      buf += node.val;
      node = node.next;
      if (node != nil)
        buf += ", ";
    }
    return buf + "]";
  }
}

var c = Node("c", nil);
var b = Node("b", c);
var a = Node("a", b);

print a.toString();